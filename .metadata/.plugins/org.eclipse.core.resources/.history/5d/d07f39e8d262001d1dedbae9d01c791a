package me.ix.noskillv2;

import java.io.File;

public class ConfigManager {

	String path = "config.cfg";
	File config_file;
	
	public ConfigManager() {
		File config_file = new File(path);
		
		if(!config_file.exists()) {
			Utils.log("Could not find config.cfg file. Generating new file.");
			
			
		}
	}
	
}
